{
  "name": "aetherius-web-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000 --strictPort",
    "dev-stable": "NODE_OPTIONS=\"--max_old_space_size=4096\" vite --host 0.0.0.0 --port 3000 --no-open",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "clean": "rm -rf node_modules/.vite dist .vite",
    "restart": "./start-frontend.sh"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "pinia": "^2.1.6",
    "element-plus": "^2.4.1",
    "@element-plus/icons-vue": "^2.1.0",
    "echarts": "^5.4.3",
    "monaco-editor": "^0.44.0",
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@rushstack/eslint-patch": "^1.3.2",
    "@tsconfig/node18": "^18.2.0",
    "@types/ws": "^8.5.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.46.0",
    "eslint-plugin-vue": "^9.15.1",
    "prettier": "^3.0.0",
    "typescript": "~5.1.6",
    "vite": "^4.4.6",
    "vue-tsc": "^1.8.6"
  }
}